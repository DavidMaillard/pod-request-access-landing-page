{
    "name": "frontend-mentor-challenge",
    "version": "1.0.0",
    "description": "Front-End Mentor Challenge",
    "main": "gulpfile.js",
    "author": "David Maillard, <maillard.david23@gmail.com>",
    "copyright": "REPLACE_ME",
    "license": "UNLICENSED",
    "private": true,
    "paths": {
        "src": {
            "base": "./src/",
            "css": "./src/css/",
            "img": "./src/img/",
            "js": "./src/js/",
            "scss": "./src/scss/"
        },
        "dist": {
            "base": "./",
            "css": "./css/",
            "js": "./js/",
            "img": "./images/"
        },
        "build": {
            "base": "./build/",
            "css": "./build/css/",
            "js": "./build/js/",
            "customJs": "./build/js/custom/",
            "html": "./build/html/",
            "img": "./build/img/"
        },
        "favicon": {
            "src": "./src/img/favicon_src.png",
            "dest": "./images/site/",
            "path": "/img/site/"
        },
        "scss": [],
        "templates": "./"
    },
    "vars": {
        "siteCssName": "site.combined.min.css",
        "scssName": "app.scss",
        "cssName": "style.css",
        "siteJsName": "site.combined.min.js"
    },
    "globs": {
        "distCss": [
            "./build/css/*.css"
        ],
        "img": [
            "./images/"
        ],
        "components": [],
        "purgecss": [
            "./templates/**/*.{html,twig}"
        ],
        "purgecssWhitelist": [],
        "critical": [],
        "criticalWhitelist": [],
        "distJs": [
            "./build/js/*.js"
        ],
        "distJsDev": [
            "./build/js/*.js"
        ],
        "distCustomJs": [
            "./build/js/custom/**/*.js"
        ],
        "babelJs": [
            "./src/js/*.js"
        ],
        "jsVendors": [
            "./src/js/vendors/*.js"
        ],
        "babelCustomJs": [
            "./src/js/site/**/*.js"
        ],
        "inlineJs": [],
        "siteIcon": "./images/site/favicon.*"
    },
    "dependencies": {
        "axios": "^0.17.1",
        "eslint": "^5.3.0",
        "fg-loadcss": "^1.2.0",
        "fontfaceobserver": "^2.0.5",
        "lazysizes": "^2.0.6",
        "loadjs": "^3.3.1",
        "normalize.css": "^5.0.0",
        "picturefill": "^3.0.2",
        "tiny-cookie": "^1.0.1"
    },
    "devDependencies": {
        "babel-plugin-transform-runtime": "^6.15.0",
        "babel-preset-es2015": "^6.16.0",
        "chalk": "^1.1.3",
        "critical": "^1.1.0",
        "fancy-log": "^1.2.0",
        "git-rev-sync": "^1.7.1",
        "gulp": "^3.9.0",
        "gulp-autoprefixer": "^3.1.0",
        "gulp-babel": "^6.1.2",
        "gulp-cached": "^1.1.1",
        "gulp-concat": "^2.6.0",
        "gulp-cssnano": "^2.1.2",
        "gulp-debug": "^2.1.2",
        "gulp-download": "^0.0.1",
        "gulp-favicons": "^2.2.6",
        "gulp-filter": "^5.0.1",
        "gulp-fontello": "^0.5.2",
        "gulp-header": "^1.8.7",
        "gulp-if": "^2.0.1",
        "gulp-imagemin": "^3.1.1",
        "gulp-livereload": "^3.8.1",
        "gulp-load-plugins": "^1.3.0",
        "gulp-newer": "^1.2.0",
        "gulp-notify": "^3.0.0",
        "gulp-plumber": "^1.1.0",
        "gulp-postcss": "^7.0.0",
        "gulp-print": "^2.0.1",
        "gulp-purgecss": "^0.15.0",
        "gulp-rename": "^1.2.2",
        "gulp-replace": "0.5.4",
        "gulp-rev": "^7.1.0",
        "gulp-sass": "^3.1.0",
        "gulp-size": "^2.1.0",
        "gulp-sourcemaps": "^2.2.1",
        "gulp-streamify": "1.0.2",
        "gulp-uglify": "^1.5.4",
        "moment": "^2.14.1",
        "pa11y": "^4.11.0",
        "tailwindcss": "^0.3.0",
        "vinyl-source-stream": "^1.1.0",
        "babel-eslint": "^8.2.1",
        "eslint": "^4.15.0",
        "eslint-config-standard": "^10.2.1",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-html": "^3.1.1",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-node": "^5.1.1",
        "eslint-plugin-promise": "^3.5.0",
        "eslint-plugin-standard": "^3.0.1",
        "eslint-friendly-formatter": "^2.0.7",
        "eslint-config-airbnb-base": "^11.0.1"
    },
    "scripts": {
        "start": "gulp",
        "build": "gulp build",
        "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
    },
    "bugs": {
        "email": "maillard.david23@gmail.com"
    }
}
